{
   "name": "Display Two Languages",
   "manifest_version": 2,
   "permissions": [ "tabs" ],
   "short_name": "Display Two Languages",
   "version": "0.20",
   "author": "SokolovDADeveloper",
   "browser_action": {
      "default_icon": {
         "16": "icons/16.png"
      },
      "default_title": ""
   },
   "content_scripts":  [
      {
         "matches": [
            "<all_urls>"
         ],
         "js": [
            "lib/jquery-3.1.1.min.js",
            "lib/toastr.min.js"
         ],
         "css": [
            "styles/toastr.min.css"
         ],
         "run_at": "document_start",
         "all_frames": true
      },
      {
         "matches": [
            "<all_urls>"
         ],
         "js": [
            "display_two_languages.js"
         ],
         "run_at": "document_end"
      }
   ],
   "icons": {
      "128": "icons/128.png",
      "16": "icons/16.png",
      "48": "icons/48.png"
   }
}
